  object qryPatientProtocols: TADOQuery
    Connection = conPatientPrescription
    CursorType = ctStatic
    DataSource = dsqryPatientList
    Parameters = <
      item
        Name = 'person_ID'
        DataType = ftGuid
        NumericScale = 255
        Precision = 255
        Size = 16
        Value = Null
      end>
    SQL.Strings = (
      
        'SELECT DISTINCT TblStdProtocol.Name_Str AS Protocol, TblStdProto' +
        'col.Indication_str AS Indication, TblPrescription.Date_dat, TblP' +
        'rescription.prescriptionStatus_Str AS Status'
      'FROM  TblStdProtocol INNER JOIN TblRXItem ON '
      '     TblStdProtocol.Protocol_ID = TblRXItem.FrmProtocol_ID'
      '      INNER JOIN TblPrescription ON '
      '     (TblRXItem.RX_ID = TblPrescription.prescription_ID)'
      '     INNER JOIN tblPerson ON'
      '     (tblPerson.person_ID = TblPrescription.patient_ID)'
      
        'WHERE (tblPerson.person_ID=:person_ID) AND (TblPrescription.pres' +
        'criptionStatus_Str NOT LIKE '#39'**VOID%'#39')'
      
        'GROUP BY TblStdProtocol.Name_Str, TblStdProtocol.Indication_str,' +
        ' TblPrescription.Date_dat, TblPrescription.prescriptionStatus_St' +
        'r'
      'ORDER BY Date_dat DESC')
    Left = 296
    Top = 192
    object qryPatientProtocolsProtocol: TWideStringField
      FieldName = 'Protocol'
      Size = 100
    end
    object qryPatientProtocolsIndication: TWideStringField
      FieldName = 'Indication'
      Size = 350
    end
    object qryPatientProtocolsDate_dat: TDateTimeField
      FieldName = 'Date_dat'
    end
    object qryPatientProtocolsStatus: TWideStringField
      FieldName = 'Status'
      Size = 50
    end
  end
