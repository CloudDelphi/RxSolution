        Precision = 23
        Size = 16
        Value = Null
      end>
    SQL.Strings = (
      'SELECT DISTINCT TblRXItem.FrmFormulation_Str'
      
        'FROM TblRXItem INNER JOIN TblPrescription ON TblRXItem.RX_ID = T' +
        'blPrescription.prescription_ID'
      'WHERE TblPrescription.Patient_ID=:Person_ID'
      'AND (TblPrescription.Date_dat >=:@StartDate)'
      'AND (TblPrescription.Date_dat <=:@EndDate)'
      'AND TblPrescription.dispensingPosted_bol = 1')
