    SQL.Strings = (
      
        'SELECT  tblAllergy.AllergyDescription_str, tblPersonAllergy.pers' +
        'onAllergySource_Str'
      
        'FROM tblPersonAllergy INNER JOIN tblAllergy ON tblAllergy.Allerg' +
        'yCode_Str = tblPersonAllergy.personAllergyDescription_Str'
      'WHERE (person_ID = :person_ID)'
      'ORDER BY tblAllergy.AllergyDescription_str')
