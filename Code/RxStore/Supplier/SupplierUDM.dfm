  object DatabaseUpdater: TADOCommand
    CommandText = 
      'CREATE VIEW dbo.strdprc_ProductSupplierContracts'#13#10'AS'#13#10'SELECT    ' +
      ' TOP 100 PERCENT dbo.TblProductContract.*, dbo.TblProductPackSiz' +
      'e.Description_str'#13#10'FROM         dbo.TblProductContract INNER JOI' +
      'N'#13#10'                      dbo.TblProductPackSize ON dbo.TblProduc' +
      'tContract.ProductCode_ID = dbo.TblProductPackSize.ProductCode_ID' +
      #13#10'ORDER BY dbo.TblProductContract.ContractYear_str DESC'#13#10
    Connection = ADOSuppliersConnection
    ExecuteOptions = [eoExecuteNoRecords]
    Parameters = <>
    Left = 40
    Top = 88
  end
  object DatabaseCleaner: TADOCommand
    CommandText = 
      'IF EXISTS (SELECT TABLE_NAME FROM INFORMATION_SCHEMA.VIEWS'#13#10'    ' +
      '  WHERE TABLE_NAME = '#39'strdprc_ProductSupplierContracts'#39')'#13#10'   DRO' +
      'P VIEW strdprc_ProductSupplierContracts'#13#10
    Connection = ADOSuppliersConnection
    ExecuteOptions = [eoExecuteNoRecords]
    Parameters = <>
    Left = 40
    Top = 40
  end
